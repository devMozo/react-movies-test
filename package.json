{
    "name": "react-movies-test",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "bundle": "webpack",
        "start": "webpack-dev-server --mode development --open --hot",
        "build": "webpack --mode production",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:updateSnapshot": "jest --updateSnapshot",
        "test:cypress": "npm run start | cypress open"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/devMozo/react-movies-test.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/devMozo/react-movies-test/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --single-quote --write",
            "git add"
        ]
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupFiles": [
            "./app/__tests__/setupTests.js"
        ],
        "moduleDirectories": [
            "node_modules",
            "./"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "babel-jest"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/cypress/",
            "<rootDir>/node_modules/"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/app/__tests__/setupTests.js"
        ]
    },
    "homepage": "https://github.com/devMozo/react-movies-test#readme",
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.0.0",
        "cypress": "^3.3.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.8.0",
        "node-sass": "^4.13.1",
        "prettier": "^1.18.2",
        "redux-mock-store": "^1.5.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "tslint": "^5.17.0",
        "tslint-immutable": "^6.0.0",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.4",
        "webpack-dev-server": "^3.7.2"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "husky": "^2.4.1",
        "lint-staged": "^8.2.1",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-icons": "^3.7.0",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0"
    }
}
